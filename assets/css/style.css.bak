.newalpha .newalpha-item {
  margin-bottom: 30px;
}

.newalpha .newalpha-wrap img {
  /* width: 100%; */
  height: 100%;
  /* Different image height than portfolio */
  object-fit: cover;
  background: var(--text-white);
  border-radius: 05px;
}

.newalpha .newalpha-links a i {
  font-size: 50px;
}

.newalpha #newalpha-flters {
  padding: 0;
  margin: 0 auto 15px auto;
  list-style: none;
  text-align: center;
  border-radius: 50px;
  padding: 2px 15px;
}

.newalpha #newalpha-flters li {
  cursor: pointer;
  display: inline-block;
  padding: 8px 16px 10px 16px;
  font-size: 14px;
  font-weight: 600;
  line-height: 1;
  text-transform: uppercase;
  color: var(--text-white);
  background: var(--white-alpha-10);
  margin: 0 3px 10px 3px;
  transition: all 0.3s ease-in-out;
  border-radius: 4px;
  height: fit-content;
}

.newalpha #newalpha-flters li:hover,
.newalpha #newalpha-flters li.filter-active {
  background: var(--primary-green);
}

.newalpha #newalpha-flters li:last-child {
  margin-right: 0;
}

.newalpha .newalpha-wrap {
  transition: 0.3s;
  position: relative;
  overflow: hidden;
  z-index: 1;
  background: var(--color-rgba-0-0-0-0_6);
}

.newalpha .newalpha-wrap::before {
  content: "";
  background: var(--color-rgba-0-0-0-0_6);
  position: absolute;
  left: 30px;
  right: 30px;
  top: 30px;
  bottom: 30px;
  transition: all ease-in-out 0.3s;
  z-index: 2;
  opacity: 0;
}

.newalpha .newalpha-wrap .newalpha-info {
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
  z-index: 3;
  transition: all ease-in-out 0.3s;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.newalpha .newalpha-wrap .newalpha-info::before {
  display: block;
  content: "";
  width: 48px;
  height: 48px;
  position: absolute;
  top: 35px;
  left: 35px;
  border-top: 3px solid var(--text-white);
  border-left: 3px solid var(--text-white);
  transition: all 0.5s ease 0s;
  z-index: 9994;
}

.newalpha .newalpha-wrap .newalpha-info::after {
  display: block;
  content: "";
  width: 48px;
  height: 48px;
  position: absolute;
  bottom: 35px;
  right: 35px;
  border-bottom: 3px solid var(--text-white);
  border-right: 3px solid var(--text-white);
  transition: all 0.5s ease 0s;
  z-index: 9994;
}

.newalpha .newalpha-wrap .newalpha-info h4 {
  font-size: 20px;
  color: var(--text-white);
  font-weight: 600;
}

.newalpha .newalpha-wrap .newalpha-info p {
  color: var(--text-white);
  font-size: 14px;
  text-transform: uppercase;
  padding: 0;
  margin: 0;
}

.newalpha .newalpha-wrap .newalpha-links {
  text-align: center;
  z-index: 4;
}

.newalpha .newalpha-wrap .newalpha-links a {
  color: var(--text-white);
  margin: 0 2px;
  font-size: 28px;
  display: inline-block;
  transition: 0.3s;
}

.newalpha .newalpha-wrap .newalpha-links a:hover {
  color: var(--color-63eda3);
}

.newalpha .newalpha-wrap:hover::before {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  opacity: 1;
}

.newalpha .newalpha-wrap:hover .newalpha-info {
  opacity: 1;
}

.newalpha .newalpha-wrap:hover .newalpha-info::before {
  top: 15px;
  left: 15px;
}

.newalpha .newalpha-wrap:hover .newalpha-info::after {
  bottom: 15px;
  right: 15px;
}





