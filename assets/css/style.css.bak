/* Optimized for maintainability â€“ visual output unchanged */
:root {
  --accent-green: #12D640;
  --background-dark: #09203a;
  --background-darker: #010e1b;
  --black-alpha-70: rgba(0, 0, 0, 0.7);
  --color-042b56eb: #042b56eb;
  --color-042e5f: #042e5f;
  --color-15bb62: #15bb62;
  --color-1f1f1f: #1f1f1f;
  --color-28a745: #28a745;
  --color-35e888: #35e888;
  --color-444444: #444444;
  --color-45a049: #45a049;
  --color-47aeff: #47aeff;
  --color-4caf50: #4CAF50;
  --color-5578ff: #5578ff;
  --color-63eda3: #63eda3;
  --color-999: #999;
  --color-ccc: #ccc;
  --color-e80368: #e80368;
  --color-ed3c0d: #ed3c0d;
  --color-eee: #eee;
  --color-f1081f: #f1081f;
  --color-ffbb2c: #ffbb2c;
  --color-gold: #9e7f25;
  --color-rgba-0-0-0-0_2: rgba(0,0,0,0.2);
  --color-rgba-0-0-0-0_4: rgba(0, 0, 0, 0.4);
  --color-rgba-0-0-0-0_6: rgba(0, 0, 0, 0.6);
  --color-rgba-18-214-64-0_1: rgba(18, 214, 64, 0.1);
  --color-rgba-18-214-64-0_6: rgba(18, 214, 64, 0.6);
  --color-rgba-255-255-255-0_11: rgba(255, 255, 255, 0.11);
  --color-transparent: transparent;
  --color-yellow: #ffc107;
  --dark-alpha-60: rgba(9, 9, 9, 0.6);
  --primary-green: #1c7d32;
  --text-gray-light: #dee2e6;
  --text-white: #ffffff;
  --transparent-alpha: rgba(0, 0, 0, 0);
  --white-alpha-08: rgba(255, 255, 255, 0.08);
  --white-alpha-10: rgba(255, 255, 255, 0.1);
  --white-alpha-12: rgba(255, 255, 255, 0.12);
  --white-alpha-15: rgba(255, 255, 255, 0.15);
  --white-alpha-20: rgba(255, 255, 255, 0.2);
  --white-alpha-25: rgba(255, 255, 255, 0.25);
  --white-alpha-30: rgba(255, 255, 255, 0.3);
}

